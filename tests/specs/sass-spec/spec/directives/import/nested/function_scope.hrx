<===> input.scss
.parent {
  // This should be visible to the imported stylesheet. There's not really a
  // good reason for this, but it's the historical behavior so whatever.
  @function local() {
    @return value;
  }

  @import 'other';
}

<===> other.scss
x {
  function: local();
}

<===> output.css
.parent x {
  function: value;
}
