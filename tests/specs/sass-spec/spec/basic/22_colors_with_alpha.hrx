<===> input.scss
$x: rgb(0, 255, 255);

div {
  color: rgb(255, $blue: 0, $green: 255);
  background: rgb(123, 45, 6);
  grah: rgba(#f0e, $alpha: .5);
  blah: rgba(1,2,3,.6);
  
  floo: $x;
  bloo: rgba($x, 0.7);
  groo: $x;
  
  $x: rgb(123, 45, 6);
  
  hoo: red($x);
  moo: green($x);
  poo: blue($x);
  
  goo: mix(rgba(255, 0, 0, 0.5), #00f);
  
  boo: invert(#123456);
}

